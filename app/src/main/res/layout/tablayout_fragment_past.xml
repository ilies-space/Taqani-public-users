<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".tabscontent.past"
    android:orientation="vertical">


    <androidx.cardview.widget.CardView
        app:cardCornerRadius="16dp"
        app:cardElevation="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal|center_vertical"
        android:layout_margin="10sp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="20sp"
            android:gravity="center"
            android:orientation="vertical"
            android:padding="5dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textView1650"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" مكتمل"
                        android:textColor="@color/yellow_main"
                        android:textSize="12dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textView16650"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:text="(2019/01/01)"
                        android:textSize="10dp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="6dp"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textView1210"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=" صيانة شاشة"
                        android:textSize="20sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/textView10"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="أحمد محمود\nطلب رقم 25251"
                        android:textSize="14dp" />

                </LinearLayout>


                <androidx.cardview.widget.CardView
                    app:cardCornerRadius="16dp"
                    app:cardElevation="2dp"
                    android:layout_width="60sp"
                    android:layout_height="60sp">

                    <ImageView
                        app:srcCompat="@drawable/logo"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/gradient_end_color"
                        android:scaleType="fitXY" />
                </androidx.cardview.widget.CardView>

            </LinearLayout>


        </LinearLayout>

    </androidx.cardview.widget.CardView>
